<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.StartController">
   <center>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="17.0" prefWidth="122.0" text="ゲームタイトル" />
            <Button fx:id="startButton" mnemonicParsing="false" onAction="#onStartButtonAction" text="ゲームスタート" />
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#onExitButtonAction" text="終了" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
